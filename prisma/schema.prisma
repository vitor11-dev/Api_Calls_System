generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id         String    @id @default(cuid())
  name       String
  email      String    @unique
  password   String
  profile    String?
  created_at DateTime  @default(now())
  updated_at DateTime?

  @@map("users")
}
